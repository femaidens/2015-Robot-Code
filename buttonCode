package edu.wpi.first.wpilibj.templates;
import edu.wpi.first.wpilibj.Joystick;

/**
 *
 * @author vincentk
 */
public class ButtonProject {
    private Joystick leftStick;
    private Joystick rightStick;
    private Joystick attackController;
    private Arm forkLift;
    
    /**
     *
     * @param leftSide
     * @param rightSide
     * @param attackController
     * @param forklift
     */
    public ButtonProject(Joystick leftSide, Joystick rightSide,Joystick 
            attack, Arm lifter)
            
    {leftStick= leftSide;
        rightStick= rightSide;
        attackController= attack;
         forkLift= lifter;}
    
    public void configureButtons (){
        
     
if(attackController.getRawButton(6))
    forkLift.up();
    }
    {if(attackController.getRawButton(6))
        forkLift.down();
    
        
    }
    {
    
    }
    
        
    
    
    
      
